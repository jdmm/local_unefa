<?xml version="1.0" encoding="utf-8"?>
   <openerp>
     <data>
    <record model="ir.ui.view" id="view_unefa_evaluacion_pde_form">
        <field name="name">unefa_evaluacion_pde.form</field>
        <field name="model">unefa.evaluacion.pde</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Evaluacion">

                <sheet>
                <group>
<!--
                <field name="corte_pde_ids" />
-->
                <field name="evaluacion" />
                <field name="ponderacion" />
                <field name="fecha" />
