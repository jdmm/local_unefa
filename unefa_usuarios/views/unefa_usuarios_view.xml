<?xml version="1.0" encoding="utf-8"?>
   <openerp>

     <data>

    
    
    <record model="ir.ui.view" id="view_unefa_estudiantes_form">
        <field name="name">unefa_estudiantes.form</field>
        <field name="model">unefa.estudiantes</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="unefa_estudiantes">
                <sheet>
                    
                        <field name="image" widget='image' class="oe_avatar oe_left" options='{"preview_image": "image_medium"}'/>
                        <div class="oe_title">
                            <label for="name" string="Nombres y Apellidos" class="oe_edit_only"/>
                            <h1><field name="name"/></h1>
                            <field name="email" invisible="1"/>
                            <label for="login" class="oe_edit_only" string="Email Address"/>
                            <h2><field name="login" on_change="on_change_login(login)"/></h2>
                            <field name="active"/>
                        </div>
                    
                    
                    <group col="4" columns="4">
                       <field name="nacionalidad" select="0" placeholder="Seleccione su nacionalidad"/>
                        <field name="cedula"  required="1" placeholder="Escriba su cedula de identidad"/>
                        <field name="fecha" placeholder="seleccione" string="Fecha de Nacimiento"/>
                        <field name="sexo" select="0" placeholder="Seleccione su Sexo"/>
                        <field name="telefono_local" select="0" data-inputmask="'mask': '9999-999-9999'"/>
                        <field name="telefono" select="0" data-inputmask="'mask': '9999-999-9999'" />
                        <field name="estado_civil" select="0" placeholder="Seleccione su Estado Civil"/>
                        <field name="carrera_id" select="0"/>
                        <field name="regimen" select="0" placeholder="Seleccione el Régimen de Estudio"/>
                        <field name="pensum_id" string="Pensum" select="0"/>
                       
                    </group>
                </sheet>                
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_unefa_estudiantes_tree">
         <field name="name">unefa_estudiantes.tree</field>
        <field name="model">unefa.estudiantes</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="unefa_estudiantes">
                <field name="name" string="Nombre Completo"/>
                <field name="nacionalidad" />
                <field name="cedula" />
            </tree>
        </field>
    </record>
      <record model="ir.actions.act_window" id="action_unefa_estudiantes">
        <field name="name">Unefa Estudiantes</field>
        <field name="res_model">unefa.estudiantes</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
    
    
